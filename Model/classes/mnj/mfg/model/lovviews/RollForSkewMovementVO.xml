  Name="RollForSkewMovementVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.mfg.model.lovviews.RollForSkewMovementVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT MNJMFGFABRICINSTESTL.ROLL_NO,
       MNJMFGFABRICINSTESTL.SUPP_ROLL_NO,
       MNJMFGFABRICINSTESTL.CUT_WIDTH,
       l.SKEW_LINE_ID,
       L.PROD_L_ID
  FROM MNJ_MFG_FABRIC_INS_TEST_L MNJMFGFABRICINSTESTL,
      MNJ_SKEW_MOVEMENT L
     
 WHERE LINE_TYPE = 'P'
       AND MNJMFGFABRICINSTESTL.PROD_L_ID =L.PROD_L_ID
      -- AND MNJMFGFABRICINSTESTL.PROD_L_ID=8956
      -- and  l.SKEW_LINE_ID=67
      -- and MNJMFGFABRICINSTESTL.roll_no='78699-CGL19'
      AND NOT EXISTS
                  (SELECT *
                     FROM MNJ_SKEW_MOVEMENT_D msd, MNJ_SKEW_MOVEMENT ms
                    WHERE     1 = 1
                          AND msd.roll_no = MNJMFGFABRICINSTESTL.roll_no and msd.SKEW_LINE_ID=ms.SKEW_LINE_ID
                          
                          and L.PROD_L_ID=ms.prod_L_id
                          and L.SKEW_LINE_ID=msd.SKEW_LINE_ID
                          
                          )]]>
  </SQLQuery>
