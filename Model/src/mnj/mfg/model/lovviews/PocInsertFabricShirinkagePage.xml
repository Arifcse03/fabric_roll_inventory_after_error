    <![CDATA[SELECT DISTINCT MNJMFGFABINSINVLDEO.DETAIL_ID,
                mmfl.FAB_OUNCE,
                MMFL.FAB_COMP composition,
                MMFL.FAB_CONST construction,
                MNJMFGFABINSINVLDEO.LINE_ID,
                (SELECT CUSTOMER_NAME BUYER_NAME
                FROM AR_CUSTOMERS
                WHERE CUSTOMER_ID = POC.BUYER_ID)
                BUYER_NAME,
                POC.SYSTEM_ID,
                POC.POC_ID,
                POC.LC_UNIT,
                MNJMFGFABINSINVLDEO.BUYER_ID,
                POC.SEASON,
                POC.STYLE,
                POC.WASH_NAME,
                POC.COLOR
  FROM MNJ_MFG_FABINS_INVL_D MNJMFGFABINSINVLDEO,
       MNJ_MFG_FABINS_INVL MMFL,
       MNJ_MFG_FABRIC_INS_TEST_H MMFH,
      XX_ORDER_INFO_V POC
 WHERE 
        MNJMFGFABINSINVLDEO.POC_ID = POC.POC_ID
       AND MNJMFGFABINSINVLDEO.LINE_ID = MMFL.LINE_ID
       --and MMFH.ORG_ID=POC.LC_UNIT
       AND MMFH.HEADER_ID = MMFL.HEADER_ID
       AND MMFH.INVOICE_NO = :p_recpt_no_s
       AND MMFH.HEADER_NO = :Doc_no]]>
